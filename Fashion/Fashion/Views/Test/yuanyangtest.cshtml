                                @for (int i = 50; i < 120; i++)
                                {
                                    <li>@i </li>
                                }
                            </ul>
                        </div>
                    </td>
                    <td>cm</td>
                </tr>
                <tr>
                    <td>臂&nbsp;&nbsp;&nbsp;&nbsp;围：</td>
                    <td>
                        <div id="ArmGirth_Choose">
                            <input type="text" name="ArmGirth" />
                            <ul>
                                @for (int i = 20; i < 80; i++)
                                {
                                    <li>@i </li>
                                }
                            </ul>
                        </div>
                    </td>
                    <td>cm</td>
                </tr>

                <tr>
                    <td>腿&nbsp;&nbsp;&nbsp;&nbsp;长：</td>
